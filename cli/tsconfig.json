{
    "compilerOptions": {
        "outDir": "./dist/",

        "module": "amd",
		"moduleResolution": "node",
		"noImplicitAny": false,
        "target": "es5",
        "lib": ["es5", "es2015", "dom", "scripthost"],
        "sourceMap": true,
        "jsx": "react",
        "skipLibCheck": true
    },
    "include": [
        "./src/**/*",
        "./lib/**/*"
    ]
}
