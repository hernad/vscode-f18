{
  "name": "vscode-f18",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.30.0"
  },
  "publisher": "hernad",
  "activationEvents": [
    "onCommand:f18.start.fin",
    "onCommand:f18.start.kalk",
    "onCommand:f18.start.pos"
  ],
  "main": "./build/ext/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "f18.start.pos",
        "title": "start POS",
        "category": "F18"
      },
      {
        "command": "f18.start.fin",
        "title": "start FIN",
        "category": "F18"
      },
      {
        "command": "f18.start.kalk",
        "title": "start KALK",
        "category": "F18"
      }
    ]
  },
  "dependencies": {
    "apollo-boost": "^0.1.22",
    "apollo-client": "^2.4.7",
    "graphql": "^14.0.2",
    "react": "^16.6.3",
    "react-apollo": "^2.3.2",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.6.3",
    "shelljs": "^0.8.3",
    "vscode": "^1.1.26"
  },
  "scripts": {
    "vscode:prepublish": "cd cli && webpack && cd .. && tsc -p tsconfig.json",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "start": "react-scripts-ts start",
    "build": "cd cli && webpack && cd .. && tsc -p tsconfig.json",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.2",
    "@types/react": "^16.7.12",
    "@types/react-dom": "^16.0.11",
    "awesome-typescript-loader": "^5.2.1",
    "source-map-loader": "^0.2.4",
    "typescript": "^3.2.1"
  },
  "repository": {
        "type": "git",
        "url": "https://github.com/hernad/vscode-f18"
  }
}
